{
  "name": "@aa/timber",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "**/*.css"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "@remix-run/react": ">=2",
    "@mantine/core": ">=8",
    "@mantine/hooks": ">=8",
    "@tabler/icons-react": ">=3"
  },
  "dependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "build": "tsup src/index.ts --tsconfig tsconfig.json --dts --format esm,cjs --sourcemap --clean && mkdir -p dist/admin && cp src/admin/*.css dist/admin/ || true",
    "prepare": "npm run build"
  }
}
